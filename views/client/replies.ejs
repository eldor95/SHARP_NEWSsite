<section class="seomun_breadcrumb" style="background-image: url(assets/images/breadcrumb_bg.jpg);">
    <div class="seomun_overlay"></div>
    <div class="container">
        <div class="row">
            <div class="col-lg-6">
                <div class="breadcrumb_title">
                    <h2>Muhokamalar</h2>
                </div>
            </div>
            <div class="col-lg-6">
                <div class="breadcrumb_link">
                    <ul>
                        <li><a href="#">Home</a></li>
                        <li><a class="active" href="#">Blog</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
</section>
<div class="col-md-12m mt-5">
    <div class="d-flex align-items-center justify-content-center vh-100">
        <div class="container">
            <div class="row justify-content-center mb-4">
                <div class="col-lg-12">
                    <h5>
                        <%= result[0].message  %>
                    </h5>
                </div>
            </div>
            <div class="row justify-content-center mb-4">
                <div class="col-lg-12">
                    <div class="comments">



                        <% for (let item of result[0].INFO) {%>
                            <div class="comment d-flex mb-4 col-md-12">

                                <div class="flex-grow-1 ms-2 ms-sm-3 col-md-12">
                                    <div class="comment-meta d-flex align-items-baseline">
                                        <!-- <h6 class="me-2 mr-5">
                                            <%= item.user_ID.name %>
                                        </h6> -->
                                        <span class="text-muted"> <%= moment(item.date).format("DD-MM-YYYY  -- hh:mm:ss") %></span>
                                    </div>
                                    <div class="comment-body">
                                        <%= item.message  %>
                                    </div>



                                </div>
                            </div>
                            <% } %>





                    </div>
                </div>
            </div>

        </div>
    </div>
</div>